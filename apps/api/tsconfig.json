{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "target": "ES2019",
    "lib": [
      "ES2019"
    ],
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolvePackageJsonExports": false
  },
  "include": [
    "src",
    "generated"
  ]
}